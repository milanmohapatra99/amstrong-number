   "source": [
    "n = int(input(\"Enter the number:-\"))\n",
    "sum=0\n",
    "n1 = n\n",
    "while(n!=0):\n",
    "    dg = n%10\n",
    "    sum = sum + dg * dg * dg\n",
    "    n = n//10\n",
    "if(n1 == sum):\n",
    "    print(n1,\"is an amstrong number\")\n",
    "else:\n",
    "    print(n1,\"is not an amstrong number\")"
   ]
